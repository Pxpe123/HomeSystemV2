{
  "name": "react-electron",
  "version": "0.1.0",
  "main": "public/main.js",
  "homepage": "./",
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.19",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.11",
    "electron": "^30.0.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-iframe": "^1.8.5",
    "react-router-dom": "^6.23.1",
    "react-scripts": "5.0.1",
    "spotify-web-api-js": "^1.5.2",
